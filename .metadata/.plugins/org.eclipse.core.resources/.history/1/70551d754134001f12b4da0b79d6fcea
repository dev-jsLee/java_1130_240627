package day01;

public class VariableTest {

	public static void main(String[] args) {
		System.out.println("Hello, world.");
		// 출력문을 작성하는 이유
		// 코드를 쓰는 중간에 오류의 위치나 값을 확인하기 위해
		
		// int, long
		int num;  // 선언
		num = 10; // 값을 할당
		
		// 변수의 선언과 할당
		long numLong = 1000L;
		long numLong2 = 1000;
		
		
		
		
		
		// float, double
		// char, String
		
		

	}

}
